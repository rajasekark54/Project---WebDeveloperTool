{
  "manifest_version": 2,
  "name": "I2I Web Tool",
  "description": "I2I",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_title": "React Ext",
    "default_icon": "./logo192.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": [
    "identity",
    "storage",
    "declarativeContent",
    "tabs",
    "https://api.figma.com/*",
    "https://www.linkedin.com/*",
    "https://www.linkedin.com/in/hirex-testing-81265a1aa/",
    "<all_urls>",
    "http://*/*",
    "https://*/*",
    "*://*/*"
  ],
  "web_accessible_resources": ["*.html"]
}
